<Window x:Class="Dev2.Studio.Views.ResourceManagement.DeleteResourceDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Confirm Resource Deletion" Height="157" Width="362" 
        WindowStartupLocation="CenterOwner" WindowStyle="ThreeDBorderWindow" ShowInTaskbar="False" MinHeight="100" MinWidth="350" ResizeMode="NoResize">
    <Grid>
        <Label Name="lblMessage" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" VerticalAlignment="Stretch" Height="69" Margin="0,0,0,50">
            <TextBlock Name="tbDisplay" TextWrapping="WrapWithOverflow" AutomationProperties.AutomationId="UI_DeleteResourceText_AutoID">
                
            </TextBlock>
        </Label>
        <Button Content="No" Height="23" HorizontalAlignment="Right" Margin="0,0,12,10" Name="button1" VerticalAlignment="Bottom" Width="75" IsCancel="True" AutomationProperties.AutomationId="UI_DeleteResourceNoBtn_AutoID" />
        <Button Content="Yes" Height="23" HorizontalAlignment="Right" Margin="0,0,93,10" Name="button2" VerticalAlignment="Bottom" Width="75" IsDefault="True" Click="button2_Click" AutomationProperties.AutomationId="UI_DeleteResourceYesBtn_AutoID"/>
        <Button Content="Show Dependencies" Height="23" HorizontalAlignment="Left" Margin="12,0,0,10" Name="button3" VerticalAlignment="Bottom" Width="120" Click="button3_Click" AutomationProperties.AutomationId="UI_DeleteResourceShowDependenciesBtn_AutoID" />
        <!--<Button Name="okButton" IsDefault="True">OK</Button>
        <Button Name="cancelButton" IsCancel="True">Cancel</Button>-->
        <!--<StackPanel Grid.Row="1" Orientation="Horizontal">
        </StackPanel>-->
    </Grid>
</Window>
