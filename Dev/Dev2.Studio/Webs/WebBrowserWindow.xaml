<Window x:Class="Dev2.Studio.Webs.WebBrowserWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cs="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
    ShowInTaskbar="True"
    WindowStyle="None"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterOwner"
    AutomationProperties.AutomationId="WebBrowserWindow"
    >
    <Border BorderThickness="1" BorderBrush="#333333">
        <cs:WebView x:Name="Browser" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
    </Border>
</Window>
