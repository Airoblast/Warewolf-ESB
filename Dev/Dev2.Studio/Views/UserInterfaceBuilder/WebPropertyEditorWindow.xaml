<Window x:Class="Unlimited.Applications.BusinessDesignStudio.Views.WebPropertyEditorWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"                     
    xmlns:local="clr-namespace:Dev2.Studio.AppResources.Behaviors"
    xmlns:cs="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
    ShowInTaskbar="True"
    Name="WebPropertyEditor"
    WindowStartupLocation="CenterOwner">
    <cs:WebView x:Name="Browser" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <cs:WebView.ContextMenu>
            <ContextMenu />
        </cs:WebView.ContextMenu>
        <i:Interaction.Behaviors>
            <local:WebControlTitleBehaviour Title="{Binding Title, ElementName=WebPropertyEditor, Mode=TwoWay}" />
        </i:Interaction.Behaviors>
    </cs:WebView>    
</Window>
