<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:controls="clr-namespace:Infragistics.Windows.Controls"
	xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:igTiles="clr-namespace:Infragistics.Windows.Tiles"
	xmlns:themes="clr-namespace:Infragistics.Windows.Themes"
	xmlns:igThemes="http://infragistics.com/Themes"
	xmlns:igWindows="clr-namespace:Infragistics.Windows.Controls">

	<Color x:Key="Color_000">#FFFFFFFF</Color>
	<Color x:Key="Color_001">#FF909AA6</Color>
	<Color x:Key="Color_002">#FFc1d4ec</Color>
	<Color x:Key="Color_003">#FFbbcee6</Color>
	<Color x:Key="Color_004">#FFD4E6F5</Color>
	<Color x:Key="Color_005">#FF6A80C4</Color>
	<Color x:Key="Color_006">#FF8993A7</Color>
	<Color x:Key="Color_007">#FF1E395B</Color>
	<Color x:Key="Color_008">#FF767676</Color>
	<Color x:Key="Color_010">#FFE5E5F0</Color>
	<Color x:Key="Color_011">#FFdbebff</Color>
	<Color x:Key="Color_012">#FFc8d9ef</Color>
	<Color x:Key="Color_013">#FF859EBF</Color>
	<Color x:Key="Color_014">#FF8fa5bf</Color>
	<Color x:Key="Color_015">#FFd4e8ff</Color>
	<Color x:Key="Color_016">#FFbfd2ea</Color>
	<Color x:Key="Color_017">#FFadc1da</Color>
	<Color x:Key="Color_018">#FFb9cde5</Color>
	<Color x:Key="Color_019">#33000000</Color>
	<Color x:Key="Color_020">#99000000</Color>
	<Color x:Key="Color_021">#00FFFFFF</Color>
	<!-- ____________________________________________________________________________ -->
	<!--                                                                              -->
	<!--                             Brushes                                          -->
	<!-- ____________________________________________________________________________ -->
		
	<!--  BEGINS BRUSHES extracted with XAML Colors Extractor  -->
<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileSwapTargetBgBrushFillKey}" StartPoint="0.5,0" EndPoint="0.5,1" igThemes:ResourceWasher.WashGroup="BaseColor">
		<GradientStop Color="{StaticResource Color_019}" Offset="0"/>
		<GradientStop Color="{StaticResource Color_020}" Offset="1"/>
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileChromeFillKey}" StartPoint="0,0" EndPoint="0,1" igThemes:ResourceWasher.WashGroup="BaseColor">
				<GradientStop Offset="0" Color="{StaticResource Color_002}"/>
				<GradientStop Offset="0.25" Color="{StaticResource Color_003}"/>
				<GradientStop Offset="0.5" Color="{StaticResource Color_003}"/>
				<GradientStop Offset="1" Color="{StaticResource Color_003}"/>
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.ToolTipCenterFillKey}" StartPoint="0.5,0" EndPoint="0.5,1" igThemes:ResourceWasher.WashGroup="BaseColor">
				<GradientStop Color="{StaticResource Color_000}" Offset="0" />
				<GradientStop Color="{StaticResource Color_010}" Offset="1" />
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileAreaSplitterBackgroundFillKey}" StartPoint="0,0" EndPoint="1,0" igThemes:ResourceWasher.WashGroup="BaseColor">
				<GradientStop Offset="0" Color="{StaticResource Color_011}"/>
				<GradientStop Offset="1" Color="{StaticResource Color_012}"/>
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileAreaSplitterHorizontalBackgroundFillKey}" StartPoint="0,0" EndPoint="0,1" igThemes:ResourceWasher.WashGroup="BaseColor">
				<GradientStop Offset="0" Color="{StaticResource Color_011}"/>
				<GradientStop Offset="1" Color="{StaticResource Color_012}"/>
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnHottrackBackgroundFillKey}" StartPoint="0.5,0" EndPoint="0.5,1" igThemes:ResourceWasher.WashGroup="Hover">
				<GradientStop Offset="0" Color="{StaticResource Color_015}"/>
				<GradientStop Offset="1" Color="{StaticResource Color_016}"/>
	</LinearGradientBrush>

<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnPressedBackgroundFillKey}" StartPoint="0.5,0" EndPoint="0.5,1" igThemes:ResourceWasher.WashGroup="Pressed">
				<GradientStop Offset="0" Color="{StaticResource Color_017}"/>
				<GradientStop Offset="0.15" Color="{StaticResource Color_018}"/>
				<GradientStop Offset="0.80" Color="{StaticResource Color_018}"/>
				<GradientStop Offset="1" Color="{StaticResource Color_017}"/>
	</LinearGradientBrush>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileContentBackgroundFillKey}" Color="{StaticResource Color_000}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileBorderBrushFillKey}" Color="{StaticResource Color_001}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TilesControlBorderBrushFillKey}" Color="{StaticResource Color_001}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionFillKey}" Color="{StaticResource Color_021}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionInnerBorderFillKey}" Color="{StaticResource Color_004}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.GrayTextBrushKey}" Color="{StaticResource Color_005}" igThemes:ResourceWasher.WashGroup="Text"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnGlyphFillKey}" Color="{StaticResource Color_006}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnHottrackGlyphFillKey}" Color="{StaticResource Color_006}" igThemes:ResourceWasher.WashGroup="Hover"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnPressedGlyphFillKey}" Color="{StaticResource Color_006}" igThemes:ResourceWasher.WashGroup="Hover"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileHeaderTextFillKey}" Color="{StaticResource Color_007}" igThemes:ResourceWasher.WashGroup="Text"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.ToolTipOuterBorderFillKey}" Color="{StaticResource Color_008}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileAreaSplitterBorderBrushFillKey}" Color="{StaticResource Color_013}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileAreaSplitterGlyphBackgroundFillKey}" Color="{StaticResource Color_021}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileAreaSplitterGlyphForegroundFillKey}" Color="{StaticResource Color_021}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnBorderFillKey}"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnBackgroundFillKey}"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnHottrackBorderFillKey}" Color="{StaticResource Color_014}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

<SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnPressedBorderFillKey}" Color="{StaticResource Color_014}" igThemes:ResourceWasher.WashGroup="BaseColor"/>

	<!-- ____________________________________________________________________________ -->
	<!--                                                                              -->
	<!--                             End of Brushes                                   -->
	<!-- ____________________________________________________________________________ -->

</ResourceDictionary>