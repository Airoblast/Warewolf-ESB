		<LinearGradientBrush.GradientStops>
			<GradientStopCollection>
				<GradientStop Color="#FF656565" Offset="0" />
				<GradientStop Color="#FF7a8da7" Offset="0.05" />
				<GradientStop Color="#FF8aa6cc" Offset="0.95" />
				<GradientStop Color="#FF8aa6cc" Offset="1" />
			</GradientStopCollection>
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{x:Static igTiles:TileBrushKeys.TileCaptionBtnHottrackGlyphFillKey}" StartPoint="0,0" EndPoint="0,1" igThemes:ResourceWasher.WashGroup="Hover">
		<LinearGradientBrush.GradientStops>
			<GradientStopCollection>
				<GradientStop Color="#FF656565" Offset="0" />
				<GradientStop Color="#FF656565" Offset="0.499" />
				<GradientStop Color="#FF829ab9" Offset="0.5" />
