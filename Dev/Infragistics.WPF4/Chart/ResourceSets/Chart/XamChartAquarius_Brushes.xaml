<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
	<SolidColorBrush x:Key="StartPaletteBrush" Color="#D86CA224"/>
	<SolidColorBrush x:Key="EndPaletteBrush" Color="#D8003366"/>
  <LinearGradientBrush x:Key="ChartBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFE7E7E7" Offset="0"/>
    <GradientStop Color="#FFE7E7E7" Offset="1"/>
  	<GradientStop Color="#FFFFFFFF" Offset="0.88"/> 
  	<GradientStop Color="#FFFFFFFF" Offset="0.135"/>
  	<GradientStop Color="#FFE5EDF3" Offset="0.495"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="ChartBorderBrush" Color="#FF9DBAD3"/>
  <!--  Axes Brushes -->
  <SolidColorBrush x:Key="AxisPrimaryXStrokeBrush" Color="#FFCCCCC5"/>
  <SolidColorBrush x:Key="AxisPrimaryYStrokeBrush" Color="#FFCCCCC5"/>
  <SolidColorBrush x:Key="AxisPrimaryZStrokeBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="LabelXBrush" Color="#FF85ADEB"/>
  <SolidColorBrush x:Key="LabelYBrush" Color="#FF85ADEB"/>
  <!-- Caption Brushes -->
  <SolidColorBrush x:Key="CaptionForegroundBrush" Color="#FF003366"/>
  <!--  GridArea Brushes -->
  <LinearGradientBrush x:Key="GridAreaBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFFFFFFF" Offset="0"/>
    <GradientStop Color="#FFFFFFFF" Offset="1"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="GridlineXMajorBrush" Color="#FFBEBEBC"/>
	<SolidColorBrush x:Key="GridlineXMinorBrush" Color="#00D7D7D7"/>
  <SolidColorBrush x:Key="GridlineYMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineYMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="GridlineZMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineZMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="TickXBrush" Color="#00699BC9"/>
  <SolidColorBrush x:Key="TickYBrush" Color="#00699BC9"/>  
  <!-- Legend Brushes-->
	<LinearGradientBrush x:Key="LegendBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFFFFFFF" Offset="0"/>
		<GradientStop Color="#FFFFFFFF" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFE0E0E0" Offset="0"/>
		<GradientStop Color="#FFE0E0E0" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderSecondaryBrush" EndPoint="0.481,0.993" StartPoint="0.481,-0.015">
		<GradientStop Color="#00FFFFFF" Offset="1"/>
		<GradientStop Color="#00FFFFFF" Offset="0"/>
	</LinearGradientBrush>
  <SolidColorBrush x:Key="LegendItemForegroundBrush" Color="#FF003366"/>
  <!-- Scene Brushes -->
  <SolidColorBrush x:Key="SceneBackgroundBrush" Color="#00E5E5E5"/>
	<SolidColorBrush x:Key="Scene3DBrush" Color="#FFE5EDF3"/>
	<SolidColorBrush x:Key="SeriesStrokeBrush" Color="#66002E41"/>
	<SolidColorBrush x:Key="MarkerFillBrush" Color="#FFFF6909"/>
  <SolidColorBrush x:Key="MarkerForegroundBrush" Color="#FF5C0000"/>
  <SolidColorBrush x:Key="MarkerStrokeBrush" Color="#00939393"/>
  <!--Stripe Brushes-->
	<SolidColorBrush x:Key="StripeFillBrush" Color="#FFF4F4F3"/>
  <SolidColorBrush x:Key="StripeStrokeBrush" Color="#00223E6E"/>
</ResourceDictionary>
  