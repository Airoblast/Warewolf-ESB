<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
  <SolidColorBrush x:Key="StartPaletteBrush" Color="#D8B4FF62"/>
  <SolidColorBrush x:Key="EndPaletteBrush" Color="#D8007EFF"/>
  <LinearGradientBrush x:Key="ChartBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FF13354A" Offset="0"/>
    <GradientStop Color="#FF123449" Offset="1"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="ChartBorderBrush" Color="#00CCCCCC"/>
  <!--  Axes Brushes -->
  <SolidColorBrush x:Key="AxisPrimaryXStrokeBrush" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AxisPrimaryYStrokeBrush" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AxisPrimaryZStrokeBrush" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="LabelXBrush" Color="#FFD6EB99"/>
  <SolidColorBrush x:Key="LabelYBrush" Color="#FFD6EB99"/>
  <!-- Caption Brushes -->
  <SolidColorBrush x:Key="CaptionForegroundBrush" Color="#FFFFFFFF"/>
  <!--  GridArea Brushes -->
  <LinearGradientBrush x:Key="GridAreaBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FF123449" Offset="0"/>
    <GradientStop Color="#FF031B33" Offset="1"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="GridlineXMajorBrush" Color="#FF2C4B5E"/>
  <SolidColorBrush x:Key="GridlineXMinorBrush" Color="#3F161515"/>
  <SolidColorBrush x:Key="GridlineYMajorBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="GridlineYMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="GridlineZMajorBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="GridlineZMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="TickXBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="TickYBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="LegendBackgroundBrush" Color="#FFC2EBE3"/>
  <SolidColorBrush x:Key="LegendBorderBrush" Color="#FFFFFFFF"/>
  <LinearGradientBrush x:Key="LegendBorderSecondaryBrush" EndPoint="0.481,0.993" StartPoint="0.481,-0.015">
    <GradientStop Color="#00FFFFFF" Offset="1"/>
    <GradientStop Color="#4CFFFFFF" Offset="0"/>
    <GradientStop Color="#26FFFFFF" Offset="0.365"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="LegendItemForegroundBrush" Color="#FF10222D"/>
  <!-- Scene Brushes -->
  <SolidColorBrush x:Key="SceneBackgroundBrush" Color="#00FFFFFF"/>
  <SolidColorBrush x:Key="Scene3DBrush" Color="#FF041C34"/>
  <!--Series Brushes-->
  <LinearGradientBrush x:Key="SeriesStrokeBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#4CFFFFFF" Offset="0"/>
    <GradientStop Color="#4CFFFFFF" Offset="1"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="MarkerFillBrush" Color="#FFF57E18"/>
  <SolidColorBrush x:Key="MarkerForegroundBrush" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="MarkerStrokeBrush" Color="#00939393"/>
  <!--Stripe Brushes-->
  <SolidColorBrush x:Key="StripeFillBrush" Color="#FF2C4B5E"/>
  <SolidColorBrush x:Key="StripeStrokeBrush" Color="#004083C8"/>
</ResourceDictionary>
