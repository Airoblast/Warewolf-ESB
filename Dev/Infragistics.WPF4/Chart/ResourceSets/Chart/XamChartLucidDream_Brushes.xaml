<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
	<!-- Resource dictionary entries should be defined here. -->
  <!--ChartControl Brushes-->
	<SolidColorBrush x:Key="StartPaletteBrush" Color="#CC95CDFF"/>
  <SolidColorBrush x:Key="EndPaletteBrush" Color="#CC192031"/>
  <LinearGradientBrush x:Key="ChartBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFBABDC4" Offset="0"/>
    <GradientStop Color="#FFFFFFFF" Offset="0.4"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="ChartBorderBrush" Color="#4C364A6D"/>
  <!--  Axes Brushes -->
  <SolidColorBrush x:Key="AxisPrimaryXStrokeBrush" Color="#FF45608C"/>
  <SolidColorBrush x:Key="AxisPrimaryYStrokeBrush" Color="#FF45608C"/>
  <SolidColorBrush x:Key="AxisPrimaryZStrokeBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="LabelXBrush" Color="#FF9FABC0"/>
  <SolidColorBrush x:Key="LabelYBrush" Color="#FF9FABC0"/>
  <!-- Caption Brushes -->
  <SolidColorBrush x:Key="CaptionForegroundBrush" Color="#FF26354E"/>
  <!--  GridArea Brushes -->
  <LinearGradientBrush x:Key="GridAreaBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#00FFFFFF" Offset="0"/>
    <GradientStop Color="#00FFFFFF" Offset="1"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="GridlineXMajorBrush" Color="#19364A6D"/>
	<SolidColorBrush x:Key="GridlineXMinorBrush" Color="#00D7D7D7"/>
  <SolidColorBrush x:Key="GridlineYMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineYMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="GridlineZMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineZMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="TickXBrush" Color="#00364A6D"/>
  <SolidColorBrush x:Key="TickYBrush" Color="#00364A6D"/>  
  <!-- Legend Brushes-->
	<LinearGradientBrush x:Key="LegendBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFFFFFFF" Offset="0"/>
		<GradientStop Color="#FFEDEDED" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFC4C5CA" Offset="0"/>
		<GradientStop Color="#FFC4C5CA" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderSecondaryBrush" EndPoint="0.481,0.993" StartPoint="0.481,-0.015">
		<GradientStop Color="#4CFFFFFF" Offset="1"/>
		<GradientStop Color="#FFFFFFFF" Offset="0"/>
	</LinearGradientBrush>
  <SolidColorBrush x:Key="LegendItemForegroundBrush" Color="#FF26354E"/>
  <!-- Scene Brushes -->
  <SolidColorBrush x:Key="SceneBackgroundBrush" Color="#00FFFFFF"/>
	<SolidColorBrush x:Key="Scene3DBrush" Color="#FFD4D4D4"/>
	<SolidColorBrush x:Key="SeriesStrokeBrush" Color="#00333434"/>
	<SolidColorBrush x:Key="MarkerFillBrush" Color="#FFFF6500"/>
  <SolidColorBrush x:Key="MarkerForegroundBrush" Color="#FF26354E"/>
  <SolidColorBrush x:Key="MarkerStrokeBrush" Color="#00939393"/>
  <!--Stripe Brushes-->
	<SolidColorBrush x:Key="StripeFillBrush" Color="#FFF1F2F5"/>
  <SolidColorBrush x:Key="StripeStrokeBrush" Color="#00223E6E"/>
</ResourceDictionary>