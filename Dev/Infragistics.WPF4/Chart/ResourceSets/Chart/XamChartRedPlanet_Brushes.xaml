<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
	<!-- Resource dictionary entries should be defined here. -->
  <!--ChartControl Brushes-->
	<SolidColorBrush x:Key="StartPaletteBrush" Color="#CCFF0000"/>
  <SolidColorBrush x:Key="EndPaletteBrush" Color="#CC370D00"/>
  <LinearGradientBrush x:Key="ChartBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFFFFFFF" Offset="0.265"/>
    <GradientStop Color="#FFFFFFFF" Offset="0.785"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="ChartBorderBrush" Color="#FFCCCCCC"/>
  <!--  Axes Brushes -->
  <SolidColorBrush x:Key="AxisPrimaryXStrokeBrush" Color="#FFDCDCDC"/>
  <SolidColorBrush x:Key="AxisPrimaryYStrokeBrush" Color="#FFDCDCDC"/>
  <SolidColorBrush x:Key="AxisPrimaryZStrokeBrush" Color="#0000C4FF"/>
  <SolidColorBrush x:Key="LabelXBrush" Color="#FF8D8D8D"/>
  <SolidColorBrush x:Key="LabelYBrush" Color="#FF8D8D8D"/>
  <!-- Caption Brushes -->
  <SolidColorBrush x:Key="CaptionForegroundBrush" Color="#FF000000"/>
  <!--  GridArea Brushes -->
  <LinearGradientBrush x:Key="GridAreaBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
    <GradientStop Color="#FFFFFFFF" Offset="1"/>
    <GradientStop Color="#FFD6EBF5" Offset="0"/>
  </LinearGradientBrush>
  <SolidColorBrush x:Key="GridlineXMajorBrush" Color="#FFE7E7E7"/>
	<SolidColorBrush x:Key="GridlineXMinorBrush" Color="#00D7D7D7"/>
  <SolidColorBrush x:Key="GridlineYMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineYMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="GridlineZMajorBrush" Color="#00A6C4D6"/>
  <SolidColorBrush x:Key="GridlineZMinorBrush" Color="#0091FF00"/>
  <SolidColorBrush x:Key="TickXBrush" Color="#FFD5DAE1"/>
  <SolidColorBrush x:Key="TickYBrush" Color="#FFD5DAE1"/>  
  <!-- Legend Brushes-->
	<LinearGradientBrush x:Key="LegendBackgroundBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFFCFCFC" Offset="1"/>
		<GradientStop Color="#FFE1E1E1" Offset="0"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderBrush" EndPoint="0,1" StartPoint="0,0">
		<GradientStop Color="#FFDCDCDC" Offset="0"/>
		<GradientStop Color="#FFDCDCDC" Offset="1"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="LegendBorderSecondaryBrush" EndPoint="0.481,0.993" StartPoint="0.481,-0.015">
		<GradientStop Color="#4CFFFFFF" Offset="1"/>
		<GradientStop Color="#FFFFFFFF" Offset="0"/>
	</LinearGradientBrush>
  <SolidColorBrush x:Key="LegendItemForegroundBrush" Color="#FF364A6D"/>
  <!-- Scene Brushes -->
  <SolidColorBrush x:Key="SceneBackgroundBrush" Color="#00FFFFFF"/>
	<SolidColorBrush x:Key="Scene3DBrush" Color="#FFEEF7FB"/>
	<SolidColorBrush x:Key="SeriesStrokeBrush" Color="#00333434"/>
	<LinearGradientBrush x:Key="MarkerFillBrush" EndPoint="1,0.5" StartPoint="0,0.5">
		<GradientStop Color="#7FFF6A00" Offset="0"/>
		<GradientStop Color="#CCFF0000" Offset="1"/>
	</LinearGradientBrush>
  <SolidColorBrush x:Key="MarkerForegroundBrush" Color="#FF000000"/>
  <SolidColorBrush x:Key="MarkerStrokeBrush" Color="#00939393"/>
	<SolidColorBrush x:Key="StripeFillBrush" Color="#FFF3F5F6"/>
  <SolidColorBrush x:Key="StripeStrokeBrush" Color="#00E7E7E7"/>
</ResourceDictionary>
  